{
  "name": "react-starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint --cache --fix",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.4.1",
    "@mui/material": "^5.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.2",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": ">=6",
    "jest": "^27.5.0",
    "lint-staged": ">=10",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.18",
    "ts-jest": "^27.1.3",
    "twin.macro": "^2.8.2",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-tsconfig-paths": "^3.4.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": "yarn lint",
    "*.css": "yarn format"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "preset": "ts-jest",
    "moduleDirectories": [
      "<rootDir>/node_modules",
      "<rootDir>/src"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true,
        "babelConfig": {
          "presets": [
            "@babel/preset-env"
          ]
        }
      }
    },
    "babelMacros": {
      "twin": {
        "preset": "emotion"
      }
    }
  }
}
